{
  "name": "front-artisan-react-starter",
  "version": "0.2.0",
  "private": true,
  "dependencies": {
    "@apollo/react-hoc": "^3.1.3",
    "@apollo/react-hooks": "^3.1.3",
    "@apollo/react-testing": "^3.1.3",
    "@formatjs/intl-displaynames": "^1.2.9",
    "@formatjs/intl-pluralrules": "^1.5.1",
    "@formatjs/intl-relativetimeformat": "^4.5.9",
    "@reduxjs/toolkit": "^1.2.3",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "enzyme-adapter-react-16": "^1.15.1",
    "graphql": "^15.0.0",
    "graphql-tag": "^2.10.1",
    "intl-messageformat-parser": "^5.0.3",
    "intl-pluralrules": "^1.1.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.26.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-intl": "^4.5.3",
    "react-redux": "^7.1.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "recompose": "^0.30.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:ci": "react-scripts test --watchAll=false",
    "eject": "react-scripts eject",
    "lint:js": "npx eslint -c .eslintrc --ignore-path .eslintignore '**/*.{js,jsx}' --quiet",
    "lint:js:fix": "npx eslint -c .eslintrc --ignore-path .eslintignore  --fix '**/*.{js,jsx}' --quiet",
    "lint:css": "npx stylelint src/**/*.css",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint:js && yarn run lint:css",
      "pre-push": "yarn run lint:js && yarn run lint:css && yarn test:ci"
    }
  },
  "devDependencies": {
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "enzyme": "^3.10.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "eslit": "^6.0.0",
    "git-cz": "^4.3.1",
    "husky": "^4.2.1",
    "prettier": "^2.0.5",
    "redux-mock-store": "^1.5.4",
    "sinon": "^9.0.2",
    "stylelint": "^13.3.3",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-declaration-use-variable": "^1.7.2",
    "stylelint-prettier": "^1.1.2"
  }
}
